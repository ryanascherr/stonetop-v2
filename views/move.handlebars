<h2>All of your moves</h2>
<div class="all-playbooks">
    {{#each moves as |move|}}
    <div class="playbook-container">
        <div class="test">
            <h3>{{move.name}}</h3>
            {{{move.description}}}
        </div>
        <div>
            <a href="/origin" data-name="{{move.name}}" class="playbook"><button>Select</button></a>
        </div>
    </div>
    {{/each}}
</div>